                {% endfor %}
              </ul>



                <div id="content" class="{{ class }} jkdsiim_rr">
                  <h1>{{ heading_title }}</h1>
                  {{ text_message }}
                  <div class="buttons">
                    <div class="pull-right"><a href="{{ continue }}" class="btn btn-primary">{{ button_continue }}</a></div>
                  </div>
                  </div>

            </div>

          </div>
        </div>
      </div>
      {{ column_right }}
    </div>
    {{ content_bottom }}
  </div>
  {{ content_bottom }}
</div>

