{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <h1>{{ heading_title }}</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
    <div class="container-fluid">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-list"></i>{{ text_view }}</h3>
            </div>
            <div class="panel-body">
                <table class="table table-bordered table-hover">
                    <tr>
                        <th>{{ text_name }}</th>
                        <td>{{ name }}</td>
                    </tr>
                    <tr>
                        <th>{{ text_email }}</th>
                        <td>{{ email }}</td>
                    </tr>
                    <tr>
                        <th>{{ text_telephone }}</th>
                        <td>{{ telephone }}</td>
                    </tr>
                    <tr>
                        <th>{{ text_text }}</th>
                        <td>{{ text }}</td>
                    </tr>
                    <tr>
                        <th>{{ text_date_added }}</th>
                        <td>{{ date_added }}</td>
                    </tr>
                    <tr>
                        <th>{{ text_utm_source }}</th>
                        <td>{{ utm_source }}</td>
                    </tr>
                    <tr>
                        <th>{{ text_utm_medium }}</th>
                        <td>{{ utm_medium }}</td>
                    </tr>
                    <tr>
                        <th>{{ text_utm_campaign }}</th>
                        <td>{{ utm_campaign }}</td>
                    </tr>
                    <tr>
                        <th>{{ text_utm_content }}</th>
                        <td>{{ utm_content }}</td>
                    </tr>
                    <tr>
                        <th>{{ text_utm_term }}</th>
                        <td>{{ utm_term }}</td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
</div>
{{ footer }}